{
    "builds": [{
        "src": "vehicle_management/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.0" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vehicle_management/wsgi.py"
        }
    ]
}